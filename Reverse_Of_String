public class Reverse_Of_String {
    public static void main(String [] args){
        String s = "123456789" ;
        String reverse = "" ;
        for(int i = s.length() -1 ; i>=0 ; i--){
            reverse = reverse + s.charAt(i) ;
        }
        System.out.println("String be " + s) ;
        System.out.println("Reverse of string be " +reverse) ;
    }
}
